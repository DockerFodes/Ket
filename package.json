{
    "name": "ketdiscordbot",
    "version": "0.1.5",
    "description": "",
    "main": "build/Main.js",
    "packageManager": "yarn@3.1.1",
    "scripts": {
        "postinstall": "npx prisma db push && npx prisma generate",
        "build": "tsc",
        "start": "npm run build && npm run noCompile",
        "dev": "node --expose-gc --trace-warnings build/Main.js --dev",
        "noCompile": "node --expose-gc --trace-warnings build/Main.js ",
        "ts-mode": "ts-node-dev --ignore-watch ./ src/Main.ts"
    },
    "prisma": {
        "schema": "src/components/Prisma/schema.prisma"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/KetDiscordBot/Ket"
    },
    "author": "Heryson1616",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/KetDiscordBot/Ket/issues"
    },
    "homepage": "https://github.com/KetDiscordBot/Ket#readme",
    "keywords": [
        "Discord",
        "Eris",
        "Typescript"
    ],
    "dependencies": {
        "@discordjs/builders": "^0.11.0",
        "@prisma/client": "^3.10.0",
        "axios": "^0.24.0",
        "canvas": "^2.9.0",
        "didyoumean": "^1.2.2",
        "erela.js": "^2.3.3",
        "eris": "^0.16.1",
        "moment": "^2.29.1",
        "moment-duration-format": "^2.3.2",
        "moment-timezone": "^0.5.33",
        "prisma": "^3.10.0",
        "prompts": "^2.4.2"
    },
    "devDependencies": {
        "@types/didyoumean": "^1.2.0",
        "@types/moment-duration-format": "^2.2.3",
        "@types/node": "^16.11.26",
        "@types/prompts": "^2.0.14",
        "dotenv": "^10.0.0",
        "typescript": "^4.5.5"
    }
}
