{
    "name": "ketdiscordbot",
    "version": "0.0.4",
    "description": "",
    "main": "dist/index.js",
    "engines": {
        "npm": "^6.9.0",
        "node": "^14.0.0"
    },
    "scripts": {
        "build": "tsc",
        "postinstall": "cross-env COPY 'src\\packages\\postinstall\\index.d.ts' 'node_modules\\eris\\index.d.ts' && cross-env COPY 'src\\packages\\postinstall\\i18next.js' 'node_modules\\i18next\\dist\\cjs'",
        "start": "npm run build && npm run no-compile",
        
        "ts-mode": "ts-node-dev --ignore-watch ./ index.ts",
        "direct-mode": "cross-env NODE_ENV=production node --expose-gc --trace-warnings dist/index.js --no-menu",
        "no-compile": "cross-env NODE_ENV=production node --expose-gc --trace-warnings dist/index.js "
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/KetDiscordBot/Ket"
    },
    "author": "Heryson1616",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/KetDiscordBot/Ket/issues"
    },
    "homepage": "https://github.com/KetDiscordBot/Ket#readme",
    "keywords": [
        "discord",
        "eris",
        "typescript"
    ],
    "dependencies": {
        "@discordjs/builders": "^0.11.0",
        "@top-gg/sdk": "^3.1.2",
        "@types/i18next-fs-backend": "^1.1.2",
        "acrcloud": "^1.4.0",
        "axios": "^0.24.0",
        "bytes": "^3.1.1",
        "canvas": "^2.8.0",
