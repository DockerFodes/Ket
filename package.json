        "i18next": "^21.6.3",
        "i18next-fs-backend": "^1.1.4",
        "mem-stat": "^1.0.5",
        "moment": "^2.29.1",
        "moment-duration-format": "^2.3.2",
        "moment-timezone": "^0.5.33",
        "npm": "^8.3.0",
        "path": "^0.12.7",
        "pg": "^8.7.1",
        "prompts": "^2.4.2",
        "typescript": "^4.5.4",
